{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "redirects": [
    {
      "source": "/optimo/assets/:filePath*",
      "destination": "https://optimo-flax.vercel.app/assets/:filePath*",
      "permanent": true
    }
  ],
  "rewrites": [
    { "source": "/optimo", "destination": "https://optimo-flax.vercel.app/" },
    { "source": "/optimo/:path*", "destination": "https://optimo-flax.vercel.app/:path*" }
  ]
}
